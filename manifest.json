{
  "manifest_version": 3,
  "name": "Focused",
  "description": "Stop doomscrolling and being a gooner",
  "version": "1.0",
  "icons": {
    "512": "logo.png"
  },
  "content_scripts": [
  {
    "matches": [
      "*://*.instagram.com/*" 
    ],
    "js": ["cleaner.js"]
    }
  ],
  "background": {
    "service_worker": "cleaner.js"
  },
  "action": {
    "default_icon": {
        "512": "logo.png"
    }
  },
  "permissions": ["activeTab", "scripting"], 
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}